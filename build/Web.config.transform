<?xml version="1.0" encoding="utf-8"?>
<configuration>
   <system.web>
      <compilation>
         <buildProviders>
            <add extension=".xcst" type="Xcst.Web.Compilation.ViewPageBuildProvider, Xcst.AspNet" />
         </buildProviders>
