<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#'
   xmlns:c='http://maxtoroq.github.io/XCST'
   xmlns:a='http://maxtoroq.github.io/XCST/application'
   extension-element-prefixes='a'>

   <c:import-namespace ns='Microsoft.VisualStudio.TestTools.UnitTesting'/>

   <c:type name='Foo'>
      <c:member name='bar' display-name='The Bar'/>
   </c:type>

   <c:template name='c:initial-template'>
      <a:model as='Foo'>
         <c:variable name='name'>
            <a:display-name for='bar'/>
         </c:variable>
         <c:void value='Assert.IsTrue(CompileType(name) == typeof(string))'/>
         <c:void value='Assert.IsTrue(name == "The Bar")'/>
      </a:model>
   </c:template>

   <c:template name='expected'/>

</c:module>