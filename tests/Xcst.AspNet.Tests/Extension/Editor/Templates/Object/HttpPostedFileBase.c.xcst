<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#'
   xmlns:c='http://maxtoroq.github.io/XCST'
   xmlns:a='http://maxtoroq.github.io/XCST/application'
   extension-element-prefixes='a'>

   <c:import-namespace ns='System.Web'/>
   
   <c:type name='Foo'>
      <c:member name='f' as='HttpPostedFileBase'/>
   </c:type>

   <c:template name='c:initial-template'>
      <div>
         <a:model value='new Foo()'>
            <a:editor/>
         </a:model>
      </div>
   </c:template>

   <c:template name='expected'>
      <div>
         <div class='editor-label'>
            <label for='f'>f</label>
         </div>
         <div class='editor-field'>
            <input type='file' id='f' name='f' class='text-box single-line'/>
            <c:text> </c:text>
            <span class='field-validation-valid' data-valmsg-for='f' data-valmsg-replace='true'/>
         </div>
      </div>
   </c:template>

</c:module>