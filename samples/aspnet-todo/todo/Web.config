<?xml version="1.0"?>
<configuration>
   <system.webServer>
      <httpProtocol>
         <customHeaders>
            <add name="Access-Control-Allow-Origin" value="*"/>
            <add name="Access-Control-Allow-Methods" value="GET,HEAD,POST,PATCH,DELETE,OPTIONS"/>
            <add name="Access-Control-Allow-Headers" value="content-type"/>
         </customHeaders>
      </httpProtocol>
   </system.webServer>
</configuration>
